<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_map">

    <fragment
        android:id="@+id/navigation_preferences"
        android:name="com.example.devfest.ui.home.HomeFragment"
        android:label="@string/title_advice"
        tools:layout="@layout/fragment_prefrences" />

    <fragment
        android:id="@+id/navigation_map"
        android:name="com.example.devfest.ui.dashboard.DashboardFragment"
        android:label="@string/title_map"
        tools:layout="@layout/fragment_map" />

    <fragment
        android:id="@+id/navigation_advice"
        android:name="com.example.devfest.ui.notifications.NotificationsFragment"
        android:label="@string/title_Preferences"
        tools:layout="@layout/fragment_advices" />
</navigation>